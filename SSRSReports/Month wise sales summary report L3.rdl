  <DataSets>
    <DataSet Name="DataSet_Revenue">
      <Query>
        <DataSourceName>DS_OnlineRetailStore_DW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Quarter">
            <Value>=Parameters!Quarter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT fct.OrderProfitPerOrder , fct.Sales , d.MonthName , reg.Region,d.QuarterName
FROM     Fact_Order_Transaction fct INNER JOIN DimDate d ON fct.OrderDateKey = d.DateKey 
       
		  INNER JOIN Dim_Customer cus ON fct.CustomerKey = cus.CustomerSK
		  INNER JOIN Dim_Country cun ON cus.CountryCodeKey = cun.CountrySK
		  INNER JOIN Dim_Region reg ON cun.RegionCodeKey = reg.RegionSK
WHERE d.QuarterName = @Quarter</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
